{
  "name": "beefree-sdk-secure-auth-example",
  "version": "2.0.0",
  "description": "Secure authentication example for Beefree SDK with React & TypeScript demo app",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently --names 'SERVER,CLIENT' \"npm run server:dev\" \"npm run client\"",
    "start": "npm run build && npm run server",
    "server": "node dist/server.js",
    "server:dev": "tsx server.ts",
    "client": "vite",
    "build": "tsc -p tsconfig.server.json && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@beefree.io/sdk": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "beefree",
    "sdk",
    "authentication",
    "email-builder",
    "react",
    "typescript"
  ],
  "author": "dev@beefree.io",
  "license": "MIT"
}
