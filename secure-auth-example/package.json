{
  "name": "beefree-sdk-secure-auth-example",
  "version": "2.0.0",
  "description": "Secure authentication example for Beefree SDK with React & TypeScript demo app",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"yarn server:dev\" \"yarn dev\"",
    "server": "node dist/server.js",
    "server:dev": "tsx watch server.ts",
    "build": "vite build && tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix"
  },
  "dependencies": {
    "@beefree.io/sdk": "^9.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "vite": "^5.0.8"
  },
  "keywords": [
    "beefree",
    "sdk",
    "authentication",
    "email-builder",
    "react",
    "typescript"
  ],
  "author": "dev@beefree.io",
  "license": "MIT"
}
