# Beefree SDK Credentials (Optional fallback credentials)
# Used only if builder-specific credentials are not set below.
BEEFREE_CLIENT_ID=your_client_id_here
BEEFREE_CLIENT_SECRET=your_client_secret_here

# Server Configuration
PORT=3006

# Auth proxy URL (points to local server)
# Leave empty to default to local proxy http://localhost:3006/auth/token
# VITE_BEEFREE_AUTH_PROXY_URL=http://localhost:3006/auth/token

# Template URLs for different builders
VITE_EMAIL_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee
VITE_PAGE_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee-page
VITE_POPUP_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee-popup

# Default builder type on load (email, page, or popup)
VITE_DEFAULT_BUILDER=email

# Optional: Builder-specific Client IDs and Secrets (if different per builder type)
# If using shared credentials above, these can be left empty

# Email Builder Credentials
# EMAIL_CLIENT_ID=your_email_client_id_here
# EMAIL_CLIENT_SECRET=your_email_client_secret_here

# Page Builder Credentials
# PAGE_CLIENT_ID=your_page_client_id_here
# PAGE_CLIENT_SECRET=your_page_client_secret_here

# Popup Builder Credentials
# POPUP_CLIENT_ID=your_popup_client_id_here
# POPUP_CLIENT_SECRET=your_popup_client_secret_here

