# Auth proxy URL (points to secure-auth-example)
# Use direct URL for cross-origin requests, or comment out to use Vite proxy
# VITE_BEEFREE_AUTH_PROXY_URL=http://localhost:3000/auth/token

# Template URLs for different builders
VITE_EMAIL_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee
VITE_PAGE_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee-page
VITE_POPUP_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/m-bee-popup

# Default builder type on load (email, page, or popup)
VITE_DEFAULT_BUILDER=email

# Builder-specific Client IDs and Secrets (if different per builder type)
# If not provided, the auth service will use shared credentials from secure-auth-example

# Email Builder Credentials
VITE_EMAIL_CLIENT_ID=your_email_client_id_here
VITE_EMAIL_CLIENT_SECRET=your_email_client_secret_here

# Page Builder Credentials
VITE_PAGE_CLIENT_ID=your_page_client_id_here
VITE_PAGE_CLIENT_SECRET=your_page_client_secret_here

# Popup Builder Credentials
VITE_POPUP_CLIENT_ID=your_popup_client_id_here
VITE_POPUP_CLIENT_SECRET=your_popup_client_secret_here
