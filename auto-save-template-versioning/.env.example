# Beefree SDK Credentials (Backend Only)
# Required for the backend server to authenticate with Beefree API
BEEFREE_CLIENT_ID=your_client_id_here
BEEFREE_CLIENT_SECRET=your_client_secret_here

# Server Configuration
# Port for the backend authentication server
PORT=3008

# Frontend Configuration (Vite)
# Port for the frontend development server
VITE_PORT=8008

# Optional: Custom Auth Proxy URL
# Overrides the default local auth endpoint (e.g. if using a shared auth server)
# Defaults to /auth/token if not set
# VITE_BEEFREE_AUTH_PROXY_URL=http://localhost:3000/auth/token

# Optional: Default Template URL
# Overrides the default starting template loaded by the editor
# Defaults to https://rsrc.getbee.io/api/templates/m-bee
# VITE_BEEFREE_TEMPLATE_URL=https://rsrc.getbee.io/api/templates/one-page-agency
