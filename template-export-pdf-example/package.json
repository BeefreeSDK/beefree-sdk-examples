{
  "name": "beefree-sdk-template-export-pdf-example",
  "version": "2.0.0",
  "description": "Template export to PDF example for Beefree SDK with React & TypeScript",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names 'SERVER,CLIENT' \"npm run server\" \"npm run client\"",
    "start": "npm run build && npm run server",
    "server": "node server.js",
    "client": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix"
  },
  "keywords": [
    "beefree",
    "sdk",
    "pdf",
    "export",
    "content-services-api",
    "react",
    "typescript"
  ],
  "author": "Beefree SDK Examples",
  "license": "MIT",
  "dependencies": {
    "@beefree.io/sdk": "^9.4.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.9.3",
    "vite": "^7.1.3"
  }
}